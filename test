#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x555a5071f490 .scope module, "FormatSpecifierExample" "FormatSpecifierExample" 2 1;
 .timescale 0 0;
v0x555a5071f620_0 .var "binaryValue", 7 0;
v0x555a507680f0_0 .var "decimalValue", 7 0;
v0x555a507681d0_0 .var "floatValue", 31 0;
v0x555a50768290_0 .var "hexValue", 7 0;
v0x555a50768370_0 .var "octalValue", 7 0;
    .scope S_0x555a5071f490;
T_0 ;
    %pushi/vec4 42, 0, 8;
    %store/vec4 v0x555a5071f620_0, 0, 8;
    %pushi/vec4 42, 0, 8;
    %store/vec4 v0x555a507680f0_0, 0, 8;
    %pushi/vec4 42, 0, 8;
    %store/vec4 v0x555a50768290_0, 0, 8;
    %pushi/vec4 42, 0, 8;
    %store/vec4 v0x555a50768370_0, 0, 8;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x555a507681d0_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x555a5071f490;
T_1 ;
    %vpi_call 2 10 "$display", "Binary: %b, Decimal: %d, Hex: %h, Octal: %o", v0x555a5071f620_0, v0x555a507680f0_0, v0x555a50768290_0, v0x555a50768370_0 {0 0 0};
    %vpi_call 2 11 "$display", "Floating-point: %f", v0x555a507681d0_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
